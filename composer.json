{
    "license": "MIT",
    "name": "stollr/ecgpb-memberlist",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.0",
        "5pm-hdh/churchtools-api": "^2.0.0",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.11.1",
        "doctrine/doctrine-migrations-bundle": "^3.3.0",
        "doctrine/orm": "^2.17.1",
        "giggsey/libphonenumber-for-php": "^8.13.27",
        "knplabs/knp-paginator-bundle": "6.3.0",
        "odolbeau/phone-number-bundle": "^3.10",
        "phpoffice/phpspreadsheet": "^1.29.0",
        "stof/doctrine-extensions-bundle": "^1.10.1",
        "stollr/tcpdf-extension": "^2.0.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.21.3",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/intl": "*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/proxy-manager-bridge": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^2.1.1",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.6.5",
        "twig/extra-bundle": "^3.8.0",
        "twig/intl-extra": "^3.8.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "rector/rector": "^0.18.13",
        "symfony/maker-bundle": "^1.52",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages" : true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.0"
        }
    }
}
