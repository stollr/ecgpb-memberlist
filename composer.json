{
    "license": "MIT",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "repositories": [
        {"type": "git", "url": "https://github.com/naitsirch/churchtools-php-adapter.git"}
    ],
    "require": {
        "php": ">=7.3.0",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.6.1",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.11.2",
        "knplabs/knp-paginator-bundle": "^5.8.0",
        "naitsirch/churchtools-api-adapter": "dev-main",
        "naitsirch/tcpdf-extension": "^1.0",
        "phpoffice/phpspreadsheet": "^1.16.0",
        "sensio/framework-extra-bundle": "^6.2.6",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/intl": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^1.11.2",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.2.26",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/maker-bundle": "^1.14",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages" : true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4"
        }
    }
}
