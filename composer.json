{
    "license": "MIT",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.3.0",
        "5pm-hdh/churchtools-api": "^1.3.5",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.8.3",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.14.1",
        "knplabs/knp-paginator-bundle": "^5.9.0",
        "naitsirch/tcpdf-extension": "^1.1",
        "phpoffice/phpspreadsheet": "^1.27.1",
        "sensio/framework-extra-bundle": "^6.2.9",
        "stof/doctrine-extensions-bundle": "^1.7.1",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.19.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/intl": "*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^1.16.1",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.6.2",
        "twig/extra-bundle": "^3.5.1",
        "twig/intl-extra": "^3.5.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/maker-bundle": "^1.48",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages" : true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4"
        }
    }
}
