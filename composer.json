{
    "license": "MIT",
    "name": "stollr/ecgpb-memberlist",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.3",
        "5pm-hdh/churchtools-api": "^2.1.0",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.13.0",
        "doctrine/doctrine-migrations-bundle": "^3.3.1",
        "doctrine/orm": "^2.19.7",
        "giggsey/libphonenumber-for-php": "^8.13.45",
        "knplabs/knp-paginator-bundle": "6.3.0",
        "odolbeau/phone-number-bundle": "^3.10",
        "phpoffice/phpspreadsheet": "^2.2.2",
        "stof/doctrine-extensions-bundle": "^1.12.0",
        "stollr/tcpdf-extension": "^2.0.0",
        "symfony/console": "^7.1.4",
        "symfony/doctrine-bridge": "^7.1.4",
        "symfony/dotenv": "^7.1.3",
        "symfony/flex": "^1.21.7",
        "symfony/form": "^7.1.4",
        "symfony/framework-bundle": "^7.1.4",
        "symfony/intl": "^7.1.1",
        "symfony/monolog-bundle": "^3.10",
        "symfony/security-csrf": "^7.1.1",
        "symfony/serializer": "^7.1.4",
        "symfony/translation": "^7.1.3",
        "symfony/validator": "^7.1.4",
        "symfony/webpack-encore-bundle": "^2.1.1",
        "symfony/yaml": "^7.1.4",
        "tecnickcom/tcpdf": "^6.7.5",
        "twig/extra-bundle": "^3.13.0",
        "twig/intl-extra": "^3.13.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": ">=3.6.1",
        "rector/rector": "^0.18.13",
        "symfony/maker-bundle": "^1.61",
        "symfony/web-profiler-bundle": "^7.1.4"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages" : true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
