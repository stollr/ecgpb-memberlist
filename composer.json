{
    "license": "MIT",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "knplabs/knp-paginator-bundle": "~2.4",
        "naitsirch/tcpdf-extension": "^1.0",
        "phpoffice/phpspreadsheet": "^1.16.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/flex": "^1.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/intl": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^1.11.2",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.2.26",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages" : true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4"
        }
    }
}
